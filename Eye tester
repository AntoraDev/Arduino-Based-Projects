#include <LCD5110_Graph.h>
String readString = "";
LCD5110 lcd(8,9,10,12,11);

extern uint8_t a[];
extern uint8_t b[];
extern uint8_t s[];
extern uint8_t p[];
extern uint8_t d[];
extern uint8_t i[];

void setup()
{
  lcd.InitLCD();
  Serial.begin(9600);
}

void loop()
{
    while (Serial.available())
  {
    char c = (char)Serial.read();
    readString +=c;
  }
    if (readString.length() > 0)
  {
    if (readString == "start")
    {
      lcd.clrScr();
      lcd.drawBitmap(0,0,a,84,48);
      lcd.update();
      delay(2000);

    }
    else if (readString == "a")
    {
      lcd.clrScr();
      lcd.drawBitmap(0,0,b,84,48);
      lcd.update();
      delay(2000);
  
    }
    else if (readString == "b")
    {
      lcd.clrScr();
      lcd.drawBitmap(0,0,s,84,48);
      lcd.update();
      delay(2000);
  
    }
    else if (readString == "is")
    {
      lcd.clrScr();
      lcd.drawBitmap(0,0,p,84,48);
      lcd.update();
      delay(2000);
  
    }
    else if (readString == "p")
    {
      lcd.clrScr();
      lcd.drawBitmap(0,0,d,84,48);
      lcd.update();
      delay(2000);
  
    }
    else if (readString == "d")
    {
      lcd.clrScr();
      lcd.drawBitmap(0,0,i,84,48);
      lcd.update();
      delay(2000);
  
    }
    readString = "";
    
  }
delay (100);

}


Display Code:

